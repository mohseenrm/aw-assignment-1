{
  "name": "CHANGE THIS : Extension boilerplate",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://extensionizr.com",
  "offline_enabled": true,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "persistent": true,
    "scripts": ["js/navigation_collector.js", "src/bg/background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "activeTab",
    "history",
    "tabs",
    "webNavigation",
    "storage",
    "*://*.stackoverflow.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.stackoverflow.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "run_at": "document_end"
    }
  ]
}